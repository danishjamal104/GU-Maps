<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gumaps">

    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ExploreActivity"
            android:theme="@style/FullscreenAppTheme"
            android:screenOrientation="nosensor">

        </activity>
        <activity android:name=".EventsActivity"
            android:theme="@style/FullscreenAppTheme"/>
        <activity android:name=".ClubInfoActivity"
            android:theme="@style/FullscreenAppTheme"/>
        <activity android:name=".FacultyDeskActivity"
            android:theme="@style/FullscreenAppTheme"/>
        <activity
            android:name=".FoodCourtActivity"
            android:theme="@style/FullscreenAppTheme">

        </activity>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/FullscreenAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/FullscreenAppTheme"></activity>
    </application>

</manifest>